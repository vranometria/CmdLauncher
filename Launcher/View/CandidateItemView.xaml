<UserControl x:Class="Launcher.View.CandidateItemView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Launcher.View"
             mc:Ignorable="d" d:DesignWidth="800" Height="58" IsTabStop="False"
             >
    <Grid>
        <Label x:Name="Keyword" Content="Label" HorizontalAlignment="Left" Height="29" VerticalAlignment="Top" Width="800"/>
        <Label x:Name="FilePass" Content="Label" Margin="0,29,0,-0.4"/>

    </Grid>
</UserControl>
